    "jest-simple-reporter": "^0.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "remote-redux-devtools": "^0.5.16",
    "reselect-tools": "^0.0.6",
    "standard": "^16.0.3",
