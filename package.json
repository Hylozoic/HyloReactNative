    "react-native-tab-navigator": "^0.3.4",
    "react-native-tab-view": "^2.15.0",
    "react-native-triangle": "^0.0.9",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^9.4.0",
    "react-native-zss-rich-text-editor": "Hylozoic/react-native-zss-rich-text-editor#merge-jb-updates-for-rn-0.61.5",
    "react-navigation": "1.5.11",
    "react-navigation-is-focused-hoc": "Hylozoic/react-navigation-is-focused-hoc#multiple-set-state",
    "react-redux": "^5.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-orm": "Hylozoic/redux-orm#createclass2-dist",
    "redux-promise": "^0.5.3",
    "reselect": "^2.5.4",
