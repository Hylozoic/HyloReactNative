{
  "name": "HyloReactNative",
  "version": "2.4.10",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "android:device": "adb reverse tcp:3001 tcp:3001 && react-native run-android",
    "android:build": "yarn run android:prod && yarn run android:usb",
    "codecov": "codecov -t b30663d3-771b-4b16-b030-0a0a56b335e3",
    "android:prod": "cd android && ./gradlew assembleRelease",
    "android:usb": "adb -d install -r android/app/build/outputs/apk/app-release.apk",
    "ios": "react-native run-ios",
    "ios:devices": "cd ios && bundle exec fastlane devices",
    "ios:testflight": "cd ios && bundle exec fastlane beta",
    "ios:qa": "cd ios && bundle exec fastlane qa",
    "test": "jest",
    "coverage:diff": "node ../jest-diff-coverage && open coverage/lcov-report/index.html",
    "clean-start": "./scripts/clean-rebuild.sh",
    "configure": "./scripts/configure.sh",
    "postinstall": "(find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm) || true",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.10.3",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/netinfo": "^5.9.0",
    "@sentry/react-native": "^1.4.5",
    "cheerio": "0.22.0",
    "ent": "^2.2.0",
    "hylo-utils": "https://github.com/Hylozoic/hylo-utils#a1f23db",
    "js-htmlencode": "^0.3.0",
    "lodash": "^4.17.13",
    "moment": "^2.27.0",
    "parse": "^1.9.2",
