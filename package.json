{
  "name": "HyloReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "android:device": "adb reverse tcp:3001 tcp:3001 && react-native run-android",
    "android:build": "yarn run android:prod && yarn run android:usb",
    "codecov": "codecov -t b30663d3-771b-4b16-b030-0a0a56b335e3",
    "android:prod": "cd android && ./gradlew assembleRelease",
    "android:usb": "adb -d install -r android/app/build/outputs/apk/app-release.apk",
    "ios": "react-native run-ios",
    "ios:devices": "cd ios && bundle exec fastlane devices",
    "ios:testflight": "cd ios && bundle exec fastlane beta",
    "ios:qa": "cd ios && bundle exec fastlane qa",
    "test": "jest",
    "coverage:diff": "node ../jest-diff-coverage && open coverage/lcov-report/index.html",
    "clean-start": "./scripts/clean-rebuild.sh",
    "configure": "./scripts/configure.sh",
    "postinstall": "(find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm) || true"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "ent": "^2.2.0",
    "hylo-utils": "Hylozoic/hylo-utils#20cd2c8",
    "js-htmlencode": "^0.3.0",
    "lodash": "^4.17.4",
    "parse": "^1.9.2",
    "path-match": "^1.2.4",
    "path-to-regexp": "^2.0.0",
    "prop-types": "^15.6.0",
    "re-reselect": "^2.0.0",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.4",
    "react-native-autocomplete-input": "^3.5.0",
    "react-native-background-timer": "^1.3.0",
    "react-native-device-info": "^0.21.5",
    "react-native-document-picker": "^2.1.0",
    "react-native-fbsdk": "^0.6.3",
    "react-native-gesture-handler": "^1.0.0-alpha.41",
    "react-native-google-signin": "https://github.com/njt1982/react-native-google-signin#1c99bcf",
    "react-native-htmlview": "^0.12.0",
    "react-native-image-picker": "^0.26.7",
    "react-native-intercom": "^12.0.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-mixpanel": "^1.1.1",
    "react-native-onesignal": "^3.1.0",
    "react-native-prompt-android": "^0.3.1",
    "react-native-restart": "^0.0.4",
    "react-native-root-toast": "^3.0.1",
    "react-native-sentry": "^0.35.3",
    "react-native-tab-navigator": "^0.3.4",
    "react-native-tab-view": "^0.0.74",
    "react-native-triangle": "^0.0.9",
    "react-native-vector-icons": "^4.5.0",
    "react-native-webview-bridge-updated": "Hylozoic/react-native-webview-bridge#62f520e",
    "react-native-zss-rich-text-editor": "Hylozoic/react-native-zss-rich-text-editor#7c1b9c5",
    "react-navigation": "^1.2",
    "react-navigation-is-focused-hoc": "Hylozoic/react-navigation-is-focused-hoc#multiple-set-state",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-orm": "Hylozoic/redux-orm#createclass2-dist",
    "redux-promise": "^0.5.3",
    "sails.io.js": "1.1.10",
    "socket.io-client": "^2.0.3",
    "striptags": "~2",
    "tinycolor2": "^1.4.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-remove-style": "^0.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "faker": "^4.1.0",
    "jest": "^22.4.3",
    "jest-simple-reporter": "^0.1.0",
    "react-test-renderer": "^16.3.0-alpha.1",
    "remote-redux-devtools": "^0.5.12",
    "reselect-tools": "^0.0.6",
    "standard": "^11.0.1",
    "timezone-mock": "^0.0.6",
    "validator": "^8.0.0",
    "why-did-you-update": "^0.1.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/config/jest/mockGlobals.js",
      "<rootDir>/config/jest/mockError.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "timers": "fake",
    "transformIgnorePatterns": [
      "/!node_modules\\/react-navigation"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "__DEV__",
      "afterAll",
      "afterEach",
      "alert",
      "beforeAll",
      "beforeEach",
      "expect",
      "fetch",
      "it",
      "describe",
      "jest",
      "test"
    ]
  }
}
